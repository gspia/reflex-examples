Name:          backend-ws-chat2
Version:       0.1.0.0
Build-type:    Simple
Cabal-version: >= 1.24

executable backend-ws-chat2
  Hs-source-dirs: src
  Ghc-options:    -Wall
  Main-is:        server.hs
  default-language: Haskell2010

  Build-depends:
    base               >= 4     && < 5,
    common-ws-chat2    ,
    aeson              ,
    bytestring         ,
    text               ,
    websockets         
    --snap-core          ,
    --snap-server        ,

